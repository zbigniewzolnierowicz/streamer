{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "outputs": ["dist/**"]
    },
    "auth#dev": {
      "dependsOn": ["^dev"],
      "outputs": ["dist/**"],
      "env": [
        "GOOGLE_CALLBACK_URL",
        "PORT",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "POSTGRES_HOST",
        "POSTGRES_PORT",
        "POSTGRES_USER",
        "POSTGRES_PASSWORD",
        "POSTGRES_DB",
        "JWT_SECRET"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"]
    }
  }
}